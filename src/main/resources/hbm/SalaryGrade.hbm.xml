<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dgd.entity.SalaryGrade" table="SALARY_GRADE" schema="h_db">
        <id name="grade" column="GRADE"/>
        <property name="highSalary" column="HIGH_SALARY"/>
        <property name="lowSalary" column="LOW_SALARY"/>
    </class>
</hibernate-mapping>